{
    "name": "leeto-digital-agency/coding-standard",
    "type": "phpcodesniffer-standard",
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
        "ext-simplexml": "*",
        "ext-dom": "*",
        "squizlabs/php_codesniffer": "^3.6.1"
    },
    "autoload": {
        "psr-4": {
            "Leeto\\": "Leeto/"
        }
    },
    "scripts": {
        "post-install-cmd": "vendor/bin/phpcs --config-set installed_paths ../../vendor/leeto-digital-agency/coding-standard",
        "post-update-cmd": "vendor/bin/phpcs --config-set installed_paths ../../vendor/leeto-digital-agency/coding-standard"
    }
}
